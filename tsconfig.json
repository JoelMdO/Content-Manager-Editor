{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/services/db/firebaseMain.js",
    "next-env.d.ts",
    "src/app/store.ts",
    "src/services/handle_image.js",
    "src/store/slices/count_slice.js",
    "src/store/slices/image_slice.ts",
    "test/button.test.jsx",
    "test/decript_test.js",
    "docs/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
